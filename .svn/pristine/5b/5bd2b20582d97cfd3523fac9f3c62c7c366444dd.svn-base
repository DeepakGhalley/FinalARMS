@using CORE_BOL
@model AssetRegisterVM

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="dialog-background" style="display:none" id="img">
    <div class="dialog-loading-wrapper">
        <span class="dialog-loading-icon"><img src="~/dist/img/loader.gif" style="width: 100px; height: 100px;" /></span>
    </div>
</div>
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark"></h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a asp-action="Index">Asset Register</a></li>
                    <li class="breadcrumb-item active">Details</li>
                </ol>
            </div>
        </div>
    </div>
</div>
<form class="form-horizontal" asp-action="Create" method="post">

    <div class="card card-info">
        <div class="card-header">
            <h3 class="card-title">Asset Register Details</h3>
        </div>
        <div class="card-body">
            <p>
                <a asp-action="Index" class="btn btn-outline-danger btn-lg" title="Go Back"><i class="fas fa-backward"></i> Go Back</a>
            </p> <div class="form-group row">
                <label class="col-sm-2 col-form-label">Asset No</label>
                <div class="col-sm-10">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.Assetnumber)</label>
                </div>
                
            </div>
            <div class="form-group row">
                <label asp-for="LapId" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.LapId)</label>
                </div>
                <label asp-for="DemkhongId" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.DemkhongId)</label>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="AssetCode" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.AssetCode)</label>
                </div>
                <label asp-for="AssetName" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.AssetName)</label>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="ResponsiblePerson" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.ResponsiblePerson)</label>
                </div>
                <label asp-for="IsActive" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.IsActive)</label>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="IsApproved" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.IsApproved)</label>
                </div>
                <label asp-for="Remarks" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.Remarks)</label>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="GisCode" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.GisCode)</label>
                </div>
                <label asp-for="GoodReceiveDate" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.GoodReceiveDate)</label>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="GoodReceiveBy" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.GoodReceiveBy)</label>
                </div>
                <label asp-for="CreatedBy" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.CreatedBy)</label>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="CreatedOn" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.CreatedOn)</label>
                </div>
                <label asp-for="ModifiedBy" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.ModifiedBy)</label>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="ModifiedOn" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.ModifiedOn)</label>
                </div>
                <label asp-for="AssetFunction" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.AssetFunctionNames)</label>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="AssetStatus" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.AssetStatusNames)</label>
                </div>
                <label asp-for="Section" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.SectionNames)</label>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="Supplier" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.SupplierNames)</label>
                </div>
                <label asp-for="TertiaryAccountHead" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.TertiaryAccountHeadNames)</label>
                </div>
            </div>
            <div class="form-group row">
                <label class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <a asp-action="Edit" asp-route-id="@Model.AssetId" title="Go Back" class="btn btn-primary fas fa-edit"
                       onclick="return saveme();">
                        Edit
                    </a>
                </div>
            </div>

        </div>
    </div>
</form>

