    function view(receiptId, TransactionTypeId) {
        //************************************** Select Transaction Type**************************************

        if (TransactionTypeId == 22) {
            displaymaetershifting(receiptId);
        }
        //Water services
        else if (TransactionTypeId == 2 || TransactionTypeId == 5 || TransactionTypeId == 6 || TransactionTypeId == 7 || TransactionTypeId == 8 || TransactionTypeId == 43 || TransactionTypeId == 22) {
            displayledgerdata(receiptId);
        }
        //water tax
        else if (TransactionTypeId == 19) {
            displaywatertax(receiptId);
        }
        //Miscellaneous tax
        else if (TransactionTypeId == 10 || TransactionTypeId == 12 || TransactionTypeId == 32) {

            displaymiscellaneousdata(receiptId);
        }
        else if ( TransactionTypeId >= 49) {
           
            displaymiscellaneousdatael(receiptId);
        }
        //vaccum tanker tax
        else if (TransactionTypeId == 11) {
            displayvacuumtanker(receiptId);
        }
        //Property Tax
        else if (TransactionTypeId == 20) {
            displayprintrecepitforproperty(receiptId);

        }
        //Vendor tax
        else if (TransactionTypeId == 15 || TransactionTypeId == 16 || TransactionTypeId == 17 || TransactionTypeId == 18) {
            fetchVendorRecepit(receiptId);

        }
        else if (TransactionTypeId == 13 || TransactionTypeId == 31 || TransactionTypeId == 34 || TransactionTypeId == 35 || TransactionTypeId == 36 || TransactionTypeId == 37 || TransactionTypeId == 38 || TransactionTypeId == 39 || TransactionTypeId == 40 || TransactionTypeId == 41) {
            displaytransactionsrecepit(receiptId);
        }
        //ENVIRONMENTAL CLERANCE
        else if (TransactionTypeId == 9) {
            viewEC(receiptId);
        }

    }



