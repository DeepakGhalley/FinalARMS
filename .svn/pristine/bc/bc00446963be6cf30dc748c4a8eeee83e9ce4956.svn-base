

@using CORE_BOL
@model LedgerDemandVM

@{
    ViewData["Title"] = "PaymentReceiptByReceiptNo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark"></h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a asp-action="DupliateRecepitPrint">Payment</a></li>
                    <li class="breadcrumb-item active">Dupliate Recepit Print</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<div class="card card-info">
    <div class="card-header">
        <h3 class="card-title">Search Receipt No</h3>
    </div>
    <div class="card-body">
        <div class="form-group row">
            <label class="col-sm-2 col-form-label">Receipt No</label>
            <div class="col-sm-3">
                <input class="form-control" placeholder="Search by Receipt No" id="ReceiptNoinput" />
            </div>

            <div class="col-sm-3">
                <button type="button" class="btn btn-primary" onclick="return SearchReceiptNo();">Search</button>
                <button type="button" class="btn btn-primary" onclick="return SearchECReceiptNo();">Search EC</button>
            </div>
        </div>
    </div>
</div>

<div class="card card-info" id="ReceiptDetails" style="display: none;">
    <div class="card-header">
        <h3 class="card-title">Receipt Details</h3>
    </div>
    <div class="card-body">
        <div class="form-group row">
            <table id="" class="table table-bordered table-striped">
                <thead style="background-color:mediumaquamarine">
                    <tr>
                        <th>TTIN</th>
                        <th>Tax Payer Name</th>
                        <th>Receipt No</th>
                        <th>Tax Name</th>
                        <th>Payment Mode</th>
                        <th>Receipt Year</th>
                        <th>Amount</th>
                        <th>Penalty</th>
                        <th>Total Amount</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody id="tblreceiptDetails">
                </tbody>
                <tr>
                    <td colspan="8"></td>
                    <td colspan="1">
                        <p>Grand Total: </p>
                    </td>
                    <td>
                        <label id="GrandTotal" value="" />
                    </td>
                </tr>
            </table>
        </div>
    </div>
</div>

<div class="card card-info" id="tbl_taxpayerdetails" style="display: none;">
    <div class="card-header">
        <h3 class="card-title">Tax Payer Details</h3>
    </div>

    <div class="card-body">
        <table style="width: 100%">
            <tr>
                <td colspan="2">
                    <b>Consumer No:</b>&nbsp; <label id="idconsumer" class="col-form-label" readonly />
                </td>
                <td colspan="2">
                    <b>Meter No:</b>&nbsp;<label id="idmeter" class="col-form-label" readonly />
                </td>
                <td colspan="2">
                    <b>Year:</b>&nbsp;<label id="idyear" class="col-form-label" readonly />
                </td>

            </tr>


        </table>
        <table class="table table-bordered table-striped">
            <thead style="background-color:mediumaquamarine">

                <tr>
                    <th>Sl.NO</th>
                    <th>Month</th>
                    <th>Billing address</th>

                    <th>Total Amount</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody id="tbl_body">
            </tbody>
        </table>
    </div>
</div>

