--exec [dbo].[rptDefaulterPropertyList] '21'
ALTER PROCEDURE [dbo].[rptDefaulterPropertyList]
@CalendarYearId int
AS
BEGIN
	SET NOCOUNT ON;	
	SELECT ROW_NUMBER() over(order by(select 1)) as Sl, tpp.firstName + isnull(tpp.middleName,'') + isnull(tpp.lastName,'') as TaxPayerName, case when tpp.taxPayerTypeId=1 then  tpp.cid else '-' end as CID,
	tpp.ttin as TTIN, sum(d.totalAmount) as TotalAmount
	from tblDemand d 
	inner join mstTaxPayerProfile tpp on d.taxPayerId = tpp.taxPayerId
	inner join mstCalendarYear cy on d.calendarYearId = cy.calendarYearId
	where d.calendarYearId >= @CalendarYearId and d.isPaymentMade = 0
	group by  tpp.firstName , tpp.middleName, tpp.lastName, tpp.cid, tpp.ttin, d.taxPayerId, tpp.taxPayerTypeId
end
