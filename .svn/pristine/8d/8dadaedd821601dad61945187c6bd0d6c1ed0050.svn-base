CREATE TABLE [dbo].[mstSuppliers]
(
	[supplierId] INT NOT NULL PRIMARY KEY IDENTITY, 
    [supplierCode] VARCHAR(50) NOT NULL, 
    [supplierName] VARCHAR(250) NOT NULL, 
     [licenseNo] VARCHAR(250) NOT NULL, 
    [supplierAddress] VARCHAR(250) NOT NULL, 
    [supplierContactPerson] VARCHAR(250) NULL, 
    [supplierContactNumber] VARCHAR(50) NULL, 
    [supplierFaxNumber] VARCHAR(50) NULL,
     [isActive] BIT NOT NULL, 
    [createdBy] INT NOT NULL, 
    [createdOn] DATETIME NOT NULL, 
    [modifiedBy] INT NULL, 
    [modifiedOn] DATETIME NULL, 
   
)
