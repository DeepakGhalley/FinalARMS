@using CORE_BOL
@model TaxPayerProfileModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="dialog-background" style="display:none" id="img">
    <div class="dialog-loading-wrapper">
        <span class="dialog-loading-icon"><img src="~/dist/img/loader.gif" style="width: 100px; height: 100px;" /></span>
    </div>
</div>
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark"></h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a asp-action="Index">Tax Payer Profile</a></li>
                    <li class="breadcrumb-item active">Details</li>
                </ol>
            </div>
        </div>
    </div>
</div>
<form class="form-horizontal" asp-action="Create" method="post">

    <div class="card card-info">
        <div class="card-header">
            <h3 class="card-title">Tax Payer Profile Details</h3>
        </div>
        <div class="card-body">
            <p>
                <a asp-action="Index" class="btn btn-outline-danger btn-lg" title="Go Back"><i class="fas fa-backward"></i> Go Back</a>
            </p>
            <div class="form-group row">
                <label asp-for="TaxPayerType" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.TaxPayerType)</label>
                </div>
                <label asp-for="Ttin" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.Ttin)</label>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="Cid" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.Cid)</label>
                </div>
                <label asp-for="Tpn" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.Tpn)</label>
                </div>
            </div>
            <div class="form-group row">
                <label class="col-sm-2 col-form-label">Name</label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">
                        @Html.DisplayFor(model => model.Title)
                        @Html.DisplayFor(model => model.FirstName)
                        @Html.DisplayFor(model => model.MiddleName)
                        @Html.DisplayFor(model => model.LastName)
                    </label>
                </div>
                <label asp-for="Dob" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.Dob)</label>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="Gender" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.Gender)</label>
                </div>
                <label class="col-sm-2 col-form-label">Village</label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">
                        @Html.DisplayFor(model => model.DzongkhagName)
                        @Html.DisplayFor(model => model.Gewogs)
                        @Html.DisplayFor(model => model.VillageName)
                    </label>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="CAddress" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.CAddress)</label>
                </div>
                <label asp-for="Occupation" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.Occupation)</label>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="WorkingAgency" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.WorkingAgency)</label>
                </div>
                <label asp-for="PhoneNo" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.PhoneNo)</label>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="MobileNo" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.MobileNo)</label>
                </div>
                <label asp-for="Email" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.Email)</label>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="OrganizationName" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.OrganizationName)</label>
                </div>
                <label asp-for="OrgPhone" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.OrgPhone)</label>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="Fax" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.Fax)</label>
                </div>
                <label asp-for="OrgEmail" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.OrgEmail)</label>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="ContactPerson" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.ContactPerson)</label>
                </div>
                <label asp-for="BankAccountNo" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.BankAccountNo)</label>
                </div>
            </div>
            <div class="form-group row">
                <label asp-for="BranchName" class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.BranchName)</label>
                </div>
            </div>
            <div class="form-group row">
                <label class="col-sm-2 col-form-label"></label>
                <div class="col-sm-4">
                    <a asp-action="Edit" asp-route-id="@Model.TaxPayerId" title="Go Back" class="btn btn-primary fas fa-edit"
                       onclick="return saveme();">
                        Edit
                    </a>
                </div>
            </div>

        </div>
    </div>
</form>

