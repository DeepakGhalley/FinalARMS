                 [landOwnershipId],
                 [g2cApplicationNo],
                 [readingDate],
                 [previousReadingDate],
                 [previousReading],
                 [oldWaterConnectionId],
                 [isPermanent]
        FROM     [dbo].[trnWaterConnection]
        ORDER BY [trnWaterConnectionId] ASC;
        SET IDENTITY_INSERT [dbo].[tmp_ms_xx_trnWaterConnection] OFF;
    END

