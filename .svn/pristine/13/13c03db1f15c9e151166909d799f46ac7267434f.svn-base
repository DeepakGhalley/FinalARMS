CREATE TABLE [dbo].[tblRevenueTransfer]
(
	[revenueTransferId] INT NOT NULL PRIMARY KEY IDENTITY, 
    [revenueTransferAmount] NCHAR(10) NULL, 
    [revenueTransferDate] NCHAR(10) NULL, 
    [revenueTakenBy] VARCHAR(250) NULL,
     [remarks] VARCHAR(350) NULL, 
    [createdOn] DATETIME NOT NULL DEFAULT GETDATE(), 
    [createdBy] INT NOT NULL, 
    [modifiedOn] DATETIME NULL, 
    [modifiedBy] INT NULL, 
)
