    function deleteLandOwnership(LandOwnershipId) {

        var r = confirm("Are you sure you want to deactivate?");
        if (r == true) {

        } else {
            return false;
        }

        $.ajax({
            type: "GET",
            url: '@Url.Action("RemoveOwnership", "LandDetails")',
            data: {
                LandOwnershipId: parseInt(LandOwnershipId)
            },
            contentType: 'application/json; charset=utf-8',
