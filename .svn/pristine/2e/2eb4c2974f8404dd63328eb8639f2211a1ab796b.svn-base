            <script>

                function GetAllDemands(id) {
                $.ajax({
                type: "GET",
                url: '@Url.Action("GetAllDemands", "Ledgers")',
                data: {
                    id: parseInt(id)
                },
                success: function (data) {

                console.log(data);
                    $('#mybody').empty();

                    //Changed from == 0 to >0
                    if (data.length > 0) {
                        $("#dvLedgerCardInfo").show(1000);
                        $.each(data, function (key, value) {

                            $('#mybody').append(
                                '<tr>' + '<td>' + (key + 1) + '</td>'
                                + '<td>' + value.transactionType + '</td>'
                                + '<td>' + value.demandNo + '</td>'
                                + '<td>' + value.demandAmount + '</td>'
                                + '<td>' + value.exemptionAmount + '</td>'
                                + '<td>' + value.totalAmount + '</td>'
                            //+ '<td>' + value.exemptionLetter + '</td>'
                                + '</tr>');
                        });

                    }
                    else {
                        $("#dvLedgerCardInfo").show(1000);
                        $('#mybody').append(
                            '<tr><td colspan="7" style="color: red;">No record found!</td></tr>');
                    }

                },
                    error: function () {
                    $('#img').hide();
                    alert('Error occured');
                    }
                });
    }
