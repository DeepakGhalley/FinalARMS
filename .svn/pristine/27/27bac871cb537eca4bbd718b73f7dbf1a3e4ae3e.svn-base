@model List<CORE_BOL.TaxMasterVM>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark"></h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a asp-action="Index">Collections</a></li>
                    <li class="breadcrumb-item active">Individual wise Collection</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->


<div class="card card-info">
    @if (TempData["msg"] != null)
    {
        <div class="alert alert-success alert-dismissible">
            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
            <h5><i class="icon fas fa-check"></i> Alert!</h5>
            <p>@TempData["msg"]</p>
        </div>
    }

    <div class="card-header">
        <h3 class="card-title">Individual wise Collection</h3>
    </div>

    <!-- /.card-header -->
    <div class="card-body">
        <p>
            <a asp-action="IndividualCollection" class="btn btn-outline-danger btn-lg" title="Add New">
                <b>
                    <i class="fas fa-plus"></i> Collect Individual Wise
                </b>
            </a>
            <a asp-action="IndividualCollected" class="btn btn-outline-danger btn-lg" title="Add New">
                <b>
                    <i class="fas fa-plus"></i> Individual collected Report
                </b>
            </a>
            <a asp-action="Individualwisecollection" class="btn btn-outline-danger btn-lg" title="Add New">
                <b>
                    <i class="fas fa-plus"></i> Individual wise collection Report
                </b>
            </a>
    </div>
</div>