@model CORE_BOL.Entities.MstCalendarYear

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="dialog-background" style="display:none" id="img">
    <div class="dialog-loading-wrapper">
        <span class="dialog-loading-icon"><img src="~/dist/img/loader.gif" style="width: 100px; height: 100px;" /></span>
    </div>
</div>
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark"></h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a asp-action="Index">Calendar Year</a></li>
                    <li class="breadcrumb-item active">Details</li>
                </ol>
            </div>
        </div>
    </div>
</div>
<form class="form-horizontal" asp-action="Create" method="post">

    <div class="card card-info">
        <div class="card-header">
            <h3 class="card-title">Calendar Year Details</h3>
        </div>
        <div class="card-body">
            <p>
                <a asp-action="Index" class="btn btn-outline-danger btn-lg" title="Go Back"><i class="fas fa-backward"></i> Go Back</a>
            </p>
            <div class="form-group row">
                <label class="col-sm-2 col-form-label">Calendar Year</label>
                <div class="col-sm-4">
                    <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.CalendarYear)</label>
                </div>
            </div>
                <div class="form-group row">
                    <label class="col-sm-2 col-form-label">Start Date</label>
                    <div class="col-sm-4">
                        <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.StartDate)</label>
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 col-form-label">End Date</label>
                    <div class="col-sm-4">
                        <label class="col-sm-10 col-form-label">@Html.DisplayFor(model => model.EndDate)</label>
                    </div>
                </div>

                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label"></label>
                        <div class="col-sm-4">
                            <a asp-action="Edit" asp-route-id="@Model.CalendarYearId" title="Go Back" class="btn btn-primary fas fa-edit"
                               onclick="return saveme();">
                                Edit
                            </a>
                        </div>
                    </div>

                </div>
            </div>
</form>

