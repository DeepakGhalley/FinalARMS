<script src="~/plugins/jquery/jquery.min.js"></script>

<script>
      $(function () {

          $('#LeaseActivityTypeId').change(function () {

            $('#img').show();

            $.ajax({
                type: "POST",
                url: '@Url.Action("FetchRate", "LandLeases")',
                data: {
                    id: parseInt($('#LeaseActivityTypeId').val())
                },
                success: function (data) {

                console.log(data);
                $('#lblrate').empty();
                    if (data.length > 0) {
                        $("#lblrate").val(data[0].rate);

                $('#img').hide();
                }
                else
                {
                $('#img').hide();
                }

                },
                error: function () {
                $('#img').hide();
                alert('Error occured');
                }
                });
                });

      });


    function av(avSelect) {

        var las = $('#landAcerage').val();
        $('#hdnla').val(las);

        var x = document.getElementById("BillingScheduleId").value;
        if (x = 1) {
            var one = $('#lblrate').val();
            var two = $('#hdnla').val();
            var avvy = one * two;
            $('#LeaseAmount').val(avvy);
        }
        else {
            var one = $('#lblrate').val();
            var two = $('#hdnla').val();
            var avvy = one * two;
            var as = avvy / 12;
            $('#LeaseAmount').val(as);
        }


    }

    function myFunction() {
        var x = document.getElementById("BillingScheduleId").value;

        if (x == 1) {

            $('#am').show(1000);
            var one = $('#lblrate').val();
            var two = $('#hdnla').val();
            var avvy = one * two;
            $('#LeaseAmount').val(avvy);
        }

        else {
            $('#am').hide(1000);
            var one = $('#lblrate').val();
            var two = $('#hdnla').val();
            var avvy = one * two;
            var as = avvy / 12;
            $('#LeaseAmount').val(as);
        }

    }
    function hideall(){
        $('#landLease').hide(1000);
        $('#landDetail').hide(1000);
        $('#searchPlotNo').hide(1000);
        $('#taxPayerDiv').hide(1000);
        $('#plotNoInfo').hide(1000);
        $('#ExistingLease').hide(1000);
    }

    function displayTaxPayer() {
            var cid = $('#cid').val();
            var ttin = $('#ttin').val();
                $.ajax({
                    type: "GET",
                    url: '@Url.Action("GetTaxPayer", "LandLeases")',
                    data: {
                        cid: cid,
                        ttin: ttin,
                    },
                    success: function (data) {
                        $('#searchPlotNoInput').val('');
                        $('#tptbl_body').empty();
                        if (data.length > 0) {
                            $.each(data, function (key, value) {

                                $('#tptbl_body').append(
                                    '<tr>'
                                    + '<td>' + (key + 1) + '</td>'
                                    + '<td>' + value.ttin + '</td>'
                                    + '<td>' + value.cid + '</td>'
                                    + '<td>' + value.name + '</td>'
                                    + '<td>' + value.mobileNo + '</td>'
                                    + '</tr>');
                            });
                            hideall();

                            $('#taxPayerDiv').show(1000);
                            $('#searchPlotNo').show(1000);

                            var taxPayerId = data[0].taxPayerId;
                            $('#hdnTaxPayerId').val(taxPayerId);


                        } else {
                            $('#taxPayerDiv').hide(1000);

                            swal("Please Enter valid CID or TTIN!", "", "warning");
                            hideall();
                        }

                    },

                    error: function () {
                        alert('error');
                        hideall();

                    }

                });
    }


    function searchPlotNo() {
        if ($('#searchPlotNoInput').val() == "") {
            swal('Please Enter Plot No!', '', 'error');
            $('#searchPlotNoInput').focus();
            return false;
        }
        var plotNo = $('#searchPlotNoInput').val();
        $('#plotNo').val(plotNo);
                $.ajax({
                    type: "GET",
                    url: '@Url.Action("GetPlotNo", "LandLeases")',
                    data: {
                        plotNo: plotNo,
                    },
                    success: function (data) {
                        

                        $('#ExistingLease').hide(1000);
                        if (data.length > 0) {

                            $.each(data, function (key, value) {
                                $('#plottbl_body').empty();
                                var la = value.landAcerage;

                                $('#hdnla').val(la);
                                $('#plottbl_body').append(
                                    '<tr>'
                                    + '<td>' + (key + 1) + '</td>'
                                    + '<td>' + value.plotNo + '</td>'
                                    + '<td>' + value.landAcerage + '</td>'
                                    + '<td>' + value.plotAddress + '</td>'

                                    //+ '<td>' + '<a style = "color: #007bff; cursor: pointer;" onclick = "return displayLandLease(' + value.plotNo + ',' + value.landAcerage + ',' + value.plotAddress + ',' + value.location + ',' + value.landValue + ',' + value.landPoolingRate + ',' + value.landUseSubCategoryId + ',' + value.streetNameId + ',' + value.lapId + ',' + value.propertyTypeId + ',' + value.structureAvailable + ',' + value.vacantLandTaxApplicable + ',' + value.garbageApplicable + ',' + value.streetLightApplicable + ',' + value.demkhongId + ',' + value.landTypeId + ',' + value.isApproved + ')" >Select</a >' + '</td>'
                                    + '<td>' + '<a style = "color: #007bff; cursor: pointer;" onclick = "return displayLandLease(' + value.landDetailId + ')" >Select</a >' + '</td>'
                                    + '</tr>');
                            });

                            $("#landLease").hide(1000);
                            $("#SaveLandLease").show(1000);
                            $("#SaveLand").hide(1000);
                            $('#ExistingLease').hide(1000);

                            $("#plotNoInfo").show(1000);
                            $("#landDetail").hide(1000);
                            //swal("Working ya","","success");
                        }
                        else {
                            $("#landLease").show(1000);
                            $("#SaveLandLease").hide(1000);
                            $("#SaveLand").show(1000);
                            $("#plotNoInfo").hide(1000);
                            $("#landDetail").show(1000);
                            $('#ExistingLease').hide(1000);

                            //swal("not Working ya","","error");
                        }
                        $('#landAcerage').val('');
                        $('#plotAddress').val('');
                        $('#StartDate').val('');
                        $('#EndDate').val('');
                        $('#lblrate').val('');
                        $('#LeaseApprovalNo').val('');
                        $('#ShedAmount').val(0);
                        $('#LeaseAmount').val('');
                        $('#streetName').val(0).trigger('change');
                        $('#demkhong').val(0).trigger('change');
                        $('#lapId').val(0).trigger('change');
                        $('#LeaseTypeId').val(0).trigger('change');
                        $('#LeaseActivityTypeId').val(0).trigger('change');
                        $('#BillingScheduleId').val(2).trigger('change');
                        $('#HasShed').prop('checked', value == 0);
                    },

                    error: function () {
                        swal("Error","","error");
                    }

                });

    }

