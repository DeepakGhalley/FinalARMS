<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="RDLC\rptDailyPaymentModeWiseDemandCollection.rdlc" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="RDLC\rptDailyPaymentModeWiseDemandCollection.rdlc">
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <None Update="DataSets\DataSet1.xsd">
      <Generator>MSDataSetGenerator</Generator>
      <LastGenOutput>DataSet1.Designer.cs</LastGenOutput>
    </None>
    <None Update="DataSets\DataSet2.xsd">
      <Generator>MSDataSetGenerator</Generator>
      <LastGenOutput>DataSet2.Designer.cs</LastGenOutput>
    </None>
    <None Update="DataSets\dsOnlinePaymentByFromDateToDate.xsd">
      <Generator>MSDataSetGenerator</Generator>
      <LastGenOutput>dsOnlinePaymentByFromDateToDate.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>